QMenu::item:selected:enabled{
    background: #0da4f6;
    color: #ffffff;
}

QMenu::item:selected:!enabled{
    background:transparent;
}

/*分割线*/
QMenu::separator{
    height:1px;
    background: #3b3b3b;
    margin:2px 0px 2px 0px;
}

QMenu::indicator:exclusive:checked {
     image: url(:/res/menu_selected.png);
}

QMenu::indicator:non-exclusive:checked{
     image:url(:/res/ok.png);
}

