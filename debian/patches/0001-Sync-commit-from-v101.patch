+
+extern "C"{
+#include <libavcodec/avcodec.h>
+#include <libavformat/avformat.h>
+}
+
+static AVFormatContext* pFormatContext = nullptr;
+int getRotateAngle(QString file)
+{
+    avcodec_register_all();
+    avformat_network_init();
+    int videoStream = -1;
+    if(pFormatContext != nullptr)
+        avformat_close_input(&pFormatContext);
+    avformat_open_input(&pFormatContext, file.toStdString().c_str(), nullptr, nullptr);
+    if(pFormatContext == nullptr)
+        return 0;
+    for(int i=0; i<pFormatContext->nb_streams; i++)
+    {
+        if(pFormatContext->streams[i]->codec->codec_type == AVMEDIA_TYPE_VIDEO)
+        {
+            videoStream = i;
+        }
+    }
+    if(videoStream == -1)
+    {
+        qDebug() << "find video stream error";
+        return 0;
+    }
+
+    AVDictionaryEntry *tag = NULL;
+    int   m_Rotate = -1;
+    tag = av_dict_get(pFormatContext->streams[videoStream]->metadata, "rotate", tag, 0);
+    if (tag == NULL)
+    {
+        m_Rotate = 0;
+    }
+    else
+    {
+        int angle = atoi(tag->value);
+        qDebug() << angle;
+        angle %= 360;
+        m_Rotate = angle;
+    }
+    return m_Rotate;
+}
